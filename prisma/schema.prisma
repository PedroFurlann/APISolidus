generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int            @id @default(autoincrement())
  name         String
  email        String
  password     String?
  googleId     String?
  facebookId   String?
  AccessToken  AccessToken[]
  transactions Transactions[]
}

model AccessToken {
  id     Int    @id @default(autoincrement())
  token  String
  userId Int
  user   User   @relation(fields: [userId], references: [id])
}

model Transactions {
  id       Int    @id @default(autoincrement())
  title    String
  type     String
  category String
  amount   Int
  User     User   @relation(fields: [userId], references: [id])
  userId   Int
}
